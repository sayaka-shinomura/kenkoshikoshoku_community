<%= form_with model: @effect, url: url do |f| %>
  <div class="effectform">
    <div class="flex-row">
      <div class="flex-item">効能名</div>
      <div class="flex-item"><%= f.text_field :name, autofocus: true, id:"inputTitle", class:"form-control" %></div>
    </div>
    <div class="flex-row">
      <div class="flex-item">効能概要</div>
      <div class="flex-item"><%= f.text_area :introduction, id:"inputIntroduction", class:"form-control", :size=>"30x15" %></div>
    </div>
    <div class="text-center my-3">
      <%= f.submit submit, class:"btn btn-success" %>
    </div>
  </div>
<% end %>